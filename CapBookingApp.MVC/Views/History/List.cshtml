@model List<ApplicationCore.Models.HistoryModel>


@{
    ViewData["Title"] = "Home Page";
}


<div class="rounded">
    <div class="container-fluid bg-light">
        <div class="row">
            @foreach (var h in Model)
            {
                <div class="col-6 col-lg-3 col-sm-4 col-xl-2">
                    <a asp-action="Details" asp-route-id="@h.Id">
                        @h.Id
                    </a>

                    @h.BookingDate

                </div>
            }
        </div>
    </div>
</div>