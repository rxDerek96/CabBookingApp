@model List<ApplicationCore.Models.PlaceModel>


@{
    ViewData["Title"] = "Home Page";
}


<div class="rounded">
    <div class="container-fluid bg-light">
        <div class="row">
            @foreach (var place in Model)
            {
                <div class="col-6 col-lg-3 col-sm-4 col-xl-2">
                    
                    @place.PlaceId
                    @place.PlaceName

                </div>
            }
        </div>
    </div>
</div>